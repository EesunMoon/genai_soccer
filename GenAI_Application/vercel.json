{
    "builds": [
      {
        "src": "application.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb","runtime": "python3.9"}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "application.py"
      }
    ],
    "functions": {
      "api/GenAI_Application/application.py": {
        "runtime": "python3.9"
      }
    }
  }